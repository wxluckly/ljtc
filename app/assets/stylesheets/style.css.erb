@charset "utf-8";
body{ line-height: 1;}
.inner-module{ width: 1000px; margin: 0 auto;}
.wrap-grey{ background-color: #f0f0f0;}
.right{ float: right;}
.left{ float: left;}

/* gotop
========================================================================== */
.cbbfixed {position: fixed;right: 40px;transition: bottom ease .3s;bottom: -85px;z-index: 3;cursor:pointer;}
.cbbfixed .cbbtn {width: 40px;height: 40px;display: block;background-color: #e70000;}
.cbbfixed .gotop {transition: background-color ease .3s;margin-top: 1px;}
.cbbfixed .gotop .up-icon{float:left;margin:14px 0 0 9px;width:23px;height:12px;background: url('<%= image_url "side-icon02.png" %>');}
.cbbfixed .gotop:hover {background-color:#dddddd;}
.cbbfixed .cweixin {display: none;}

/* nav
========================================================================== */
.nav{ height: 56px; width: 100%; padding-top: 42px; border-bottom: 1px solid #c8c8c8; background-color: #fff;}
.nav .logo,
.nav .login{ padding:0 10px; line-height:37px;}
.nav li a{ text-decoration:none;}
.nav li a:hover{ color:#e70000;}
.nav ul{ list-style:none;}
.navFix{background-color: #fff; position:fixed; left:0; top:0; _position:absolute; top:expression((offsetParent.scrollTop)+0); z-index:100;}
.logo{ width: 300px; font-size: 0; float: left;}
.login{ float: right; text-align: right; font-size: 0; width: 600px;}
.login .user-link a,
.login .user-link span{font-size: 20px; color: #777878; float: right;}
.login .user-link span{padding: 0 20px;}

.logged-on{ float: right;}
.user-img{ position: relative; width: 44px; height: 44px; line-height: 0; overflow: hidden; margin-right: 15px; float: left;}
.user-img .mask{ position: absolute; left: 0; top: 0; z-index: 10;}
.user-img img{ width: 44px; height: 44px;}
.user-center{ float: left; line-height: 44px; position: relative; z-index: 10; padding-right: 30px;}
.icon-down{ width:0; height:0; border-width:6px 6px 0; border-style:solid; border-color:#e70000 #fff #fff; position:relative; display: block; position: absolute; right: 0; top: 50%; margin-top: -6px;}
.user-center ul{ position: absolute; top: 44px; *top: 42px; right: 0; line-height: 0; width: 145px; padding-top: 12px; *padding-top: 14px; display: none; z-index: 10;}
.user-center ul li{ width: 100%; font-size: 0; overflow: hidden; text-align: left;  border:1px solid #c8c8c8; border-bottom-color: #f3f3f3; border-top: 0;}
.user-center ul .exit{ border-bottom:1px solid #c8c8c8;}
.user-center ul li a{display: block; font-size: 14px; color: #505050; height: 38px; line-height: 38px; padding: 0 8px 0 55px; background: #fff url('<%= image_url "icon.png" %>') 18px 6px no-repeat;}
.user-center ul li a:hover{ background-color: #e70000; color: #fff;}

.user-name{ font-size: 16px; color: #505050;}
.user-center ul .write a:hover{ background-position: 18px -78px;}

.user-center ul .my-travels a{background-position: 18px -162px;}
.user-center ul .my-travels a:hover{background-position: 18px -248px;}

.user-center ul .my-profile a{background-position: 18px -332px;}
.user-center ul .my-profile a:hover{background-position: 18px -416px;}

.user-center ul .exit a{background-position: 18px -500px;}
.user-center ul .exit a:hover{background-position: 18px -585px;}

/* banner
========================================================================== */
.banner-module{padding-bottom:40px;}
.main_visual{height:400px;border-top:1px solid #d7d7d7;overflow:hidden;position:relative;}
.main_image{height:400px;overflow:hidden;position:relative;}
.main_image ul{width:9999px;height:400px;overflow:hidden;position:absolute;top:0;left:0; background-color: #f4f4f4;}
.main_image li{float:left;width:100%;height:400px;}
.main_image li span{display:block;width:100%;height:400px}
.main_image li a{display:block;width:100%;height:400px;}
.main_image li .img_1{background:url('<%= image_url "banner2.jpg" %>') center top no-repeat}
.main_image li .img_2{background:url('<%= image_url "banner3.jpg" %>') center top no-repeat}
.main_image li .img_3{background:url('<%= image_url "bg5.jpg" %>') center top no-repeat}


div.flicking_con{position:absolute;top:370px;left:50%;z-index:10;width:300px;height:21px;margin:0 0 0 -50px;}
div.flicking_con a{float:left;width:21px;height:21px;margin:0 10px;padding:0;background:url('<%= image_url "btn_main_img.png" %>') 0 0 no-repeat;display:block;text-indent:-1000px}
div.flicking_con a.on{background-position:0 -21px}
#btn_prev,#btn_next{z-index:11111;position:absolute;display:block;width:31px!important;height:58px!important;top:50%;margin-top:-37px;display:none;}
#btn_prev{background:url('<%= image_url "prev.png" %>') no-repeat left top;left:50%;margin-left:-500px;}
#btn_next{background:url('<%= image_url "next.png" %>') no-repeat right top;right:50%;margin-right:-500px;}

/* banner-mini
========================================================================== */
.ck-slide { width: 320px; height: 120px; margin: 0 auto;}
.ck-slide ul.ck-slide-wrapper { width: 100%; height: 120px;}
.ck-slide ul { margin: 0; padding: 0; list-style-type: none;}
.ck-slide { position: relative; overflow: hidden;}
.ck-slide ul.ck-slide-wrapper { position: absolute; top: 0; left: 0; z-index: 1; margin: 0; padding: 0;}
.ck-slide ul.ck-slide-wrapper li { position: absolute; width: 100%; height: 100%;}
.ck-slide ul.ck-slide-wrapper li a { display: block; width: 100%; height: 100%;}
.ck-slide .ck-prev, .ck-slide .ck-next { display: none; position: absolute; top: 50%; z-index: 2; width: 35px; height: 70px; margin-top: -35px; border-radius: 3px; opacity: .15; background: red; text-indent: -9999px; background-repeat: no-repeat; transition: opacity .2s linear 0s;}
.ck-slide .ck-prev { left: 5px; background: url(<%= image_url "arrow-left.png" %>) #000 50% no-repeat;}
.ck-slide .ck-next { right: 5px; background: url(<%= image_url "arrow-right.png" %>) #000 50% no-repeat;}
.ck-slidebox { position: absolute; right: 10px; bottom: 12px; z-index: 30;}
.ck-slidebox ul li { float: left; height: 12px; margin: 4px 4px;}
.ck-slidebox ul li em { display: block; width: 12px; height: 12px; border-radius: 100%; background: url('<%= image_url "dote.png" %>') -25px top no-repeat; text-indent: -9999px; cursor: pointer;}
.ck-slidebox ul li.current em,
.ck-slidebox ul li em:hover { background-position: left top;}

/* hot-address
========================================================================== */
.title{ font-size: 0; margin-bottom: 30px; font-weight: normal;}
.title span{ font-size: 28px; color: #505050;}

.title-icon1,
.title-icon2,
.title-icon3{ padding-left: 70px; height: 49px; line-height: 49px;}

.title-icon1 span,
.title-icon2 span,
.title-icon3 span{ color: #fff;}

.title-icon1{ background: url(<%= image_url "title_03.png" %>) transparent left top no-repeat;}
.title-icon2{ background: url(<%= image_url "title_06.png" %>) transparent left top no-repeat;}
.title-icon3{ background: url(<%= image_url "title_08.png" %>) transparent left top no-repeat;}

.title span,
.title .more{display: block; float: left;}
.title a:hover{color: #e70000;}

.title .more{ font-size: 16px; float: right; font-weight: normal; color: #969696; padding-left: 24px; margin: 0 0 0 60px; line-height: 28px; background: url('<%= image_url "right.png" %>') no-repeat 0 center;}

.more{ font-size: 14px; font-weight: normal; color: #969696; padding-left: 24px; margin: 0 0 0 20px; line-height: 28px; background: url('<%= image_url "moremini.png" %>') no-repeat 0 center;}

.hot-address{ font-size: 0; margin-left: -20px;}
.hot-address li{ float: left; margin: -20px 0 0 20px; width: 320px;}
.hot-address li a{ display: block; margin-top: 20px; position: relative;}

.hot-address li .opcity-bg,
.hot-address li span{ display: block; position: absolute;}

.hot-address li .opcity-bg{ top: 0; left: 0; width: 320px; height: 100%; background: url('<%= image_url "opcity.png" %>') left top repeat;}

.hot-address li span{ top: 50%; left: 0; width: 320px; font-size: 34px; color: #fff; text-align: center; line-height: 1; margin-top: -17px; cursor: pointer;}

.bottom-line{ font-size: 0; line-height: 0; background-color:#dedddd; width: 100%; height:2px; margin: 30px 0;}
.bottom-line-white{ font-size: 0; line-height: 0; width: 100%; height:2px; margin: 30px 0;}
.hui-img{ padding-top: 40px;}

/* left-bar
========================================================================== */
.left-bar{ float: left; width: 660px;}
.right-bar{ float: right; width: 320px;}
.right-bar .title{ margin-bottom: 30px;}
.bottom-line1{ margin: 20px 0; width: 100%; height: 1px; background-color: #eeeeee; clear: both;}
.line-clear{ display: none;}
.list-content li .img-content { display: block; font-size: 0; line-height: 0; float: left; width: 208px; height: 124px; border:1px solid #f4f4f4;}
.list-content li img {width: 100%; height: 100%;}
.list-description{ width: 430px; float: right;}
.list-description h3 a{ font-size: 20px; font-weight: normal; color: #146cff; display: block; width: 100%; height: 22px; line-height: 22px; overflow: hidden; white-space: nowrap; text-overflow:ellipsis;}
.list-description .summary{ font-size: 14px; color: #505050; line-height: 22px; text-align: justify; height: 66px; overflow: hidden; margin: 10px 0 10px;}
.list-description .small-text{ font-size: 12px; color: #969696;}
.list-description .small-text span{ display: block; float: left; width: 90px; padding-right: 10px; height: 24px; line-height: 24px; overflow: hidden; white-space: nowrap; text-overflow:ellipsis;}
.list-description .small-text .icon-heart{ padding: 0 10px 0 20px; width: 40px; background: url('<%= image_url "heart.png" %>') no-repeat left center; color: #e70000;}
.list-description .small-text .icon-share0{ padding: 0 10px 0 20px; width: 40px; background: url('<%= image_url "share0.png" %>') no-repeat left center;}
.list-description .small-text .from{ padding-right: 0;}


/* right-bar
========================================================================== */
.active-rule{ padding: 15px 20px 10px; background-color: #f0f0f0;}
.active-rule h4{ font-size: 20px; font-weight: normal; color: #e70000; margin-bottom: 5px;}
.active-rule p{ font-size: 14px; line-height: 25px; color: #505050; text-align: justify;}
.rule-time{ margin-bottom: 10px;}
.right-bar .bottom-line{ margin: 20px 0;}
.product-link { display: block; margin-top: -20px;}

/* theme-activities
========================================================================== */
.theme-activities { margin: -20px 0 0 -20px;}
.theme-activities li{ width: 490px; margin: 20px 0 0 20px; float: left;}
.theme-content{ position: relative;}
.theme-title{ position: absolute; bottom: 20px; left: 0; font-size: 22px; color: #fff; padding: 0 20px;}
.theme-content a{ color: #fff;}


